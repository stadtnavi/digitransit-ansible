services.stadtnavi.eu {
  
  handle_path /layer-categories/* {
    header {
      Access-Control-Allow-Headers *
      Access-Control-Allow-Methods *
      Access-Control-Allow-Origin *
    }
    file_server browse
    root * /var/layer-categories/
  }

  handle /* {
    reverse_proxy localhost:{{ services_port }}
  }
}